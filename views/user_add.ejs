  <form action="/add-done" method="post" id="form">
   <div class="form-group">
       <label for="id">Id:</label>
       <input type="tel" class="form-control" name="u_id" placeholder="eg.1">
   </div>
<div class="form-group">
    <label for="f_name">First Name:</label>
    <input type="tel" class="form-control" name="f_name" placeholder="eg.xyz">
</div>
<div class="form-group">
    <label for="l_name">Last Name:</label>
    <input type="tel" class="form-control" name="l_name" placeholder="eg.abc">
</div>
